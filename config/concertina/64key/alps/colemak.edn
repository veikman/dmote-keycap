            :faces {:top {:char "‚éà" :text-options {:style {:font-size "3.4mm"} :y 1}}
                    :south {:char "]"}}}}
  {:legend {:filename-suffix "right-shift"
            :faces {:top {:char "‚áß" :text-options {:style {:font-size "3.4mm"} :y 1}}
                    :north {:char ")"}}}}]}
